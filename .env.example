# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Your Telegram User ID (for private bot access, optional)
ADMIN_USER_ID=your_telegram_user_id

# Supabase PostgreSQL
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key

# Antigravity API (get from Antigravity OAuth flow)
ANTIGRAVITY_CLIENT_ID=your_client_id.apps.googleusercontent.com
ANTIGRAVITY_CLIENT_SECRET=your_client_secret
ANTIGRAVITY_EMAIL=your_email@gmail.com
ANTIGRAVITY_REFRESH_TOKEN=your_refresh_token
ANTIGRAVITY_PROJECT_ID=your_project_id

# ElevenLabs TTS (optional - for voice responses)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_VOICE_ID=iP95p4xoKVk53GoZ742B

# Vercel URL (for webhook setup)
VERCEL_URL=your-app.vercel.app
