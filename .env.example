# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Your Telegram User ID (optional - restricts bot to you only)
# Get it from @userinfobot in Telegram
ADMIN_USER_ID=your_telegram_user_id

# Supabase PostgreSQL
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key

# Antigravity OAuth (get from antigravity.ai)
# NOTE: This bot uses Antigravity OAuth, NOT Google Cloud API keys!
ANTIGRAVITY_CLIENT_ID=your_client_id.apps.googleusercontent.com
ANTIGRAVITY_CLIENT_SECRET=GOCSPX-your_client_secret
ANTIGRAVITY_EMAIL=your_email@gmail.com
ANTIGRAVITY_REFRESH_TOKEN=1//your_refresh_token
ANTIGRAVITY_PROJECT_ID=your-project-id

# ElevenLabs TTS (optional - for voice responses)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_VOICE_ID=iP95p4xoKVk53GoZ742B
